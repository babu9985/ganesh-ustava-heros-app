{
  "expo": {
    "name": "ganesh-ustava-heros",
    "slug": "ganesh-ustava-heros",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ganeshlogo2.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ganeshlogo2.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ganeshlogo2.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ganeshdonda.ganeshustavaheros"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-image-picker",
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
